{
  "program": "ans_registry.aleo",
  "version": "1.0.0",
  "description": "Aleo Name Service (ANS) registry for human-readable to address resolution.",
  "developer": "PNW Engineering Team",
  "license": "Proprietary",
  "functions": {
    "register_ans_name": {
      "inputs": ["[u8]", "address"],
      "outputs": ["bool"]
    },
    "resolve_ans_name": {
      "inputs": ["[u8]"],
      "outputs": ["address"]
    },
    "reverse_lookup_address": {
      "inputs": ["address"],
      "outputs": ["[u8]"]
    }
  }
}
